<COMPONENT>
  <!-- sDatastoreIn is used to transfert the content of a datastore
  to the next component. 
  
  sDatastoreIn should be used in templates, following one component doing process
  generating a datastore. Then this datastore is passed to sDatastoreIn to copy 
  feature to output flow.
  
  eg.
  <TEMPLATES INPUT="AGG" OUTPUT="DI">
      <TEMPLATE NAME="AGG" COMPONENT="sVectorRasterOut">
        <LINK_TO NAME="DI" CTYPE="ROWS_END" />
      </TEMPLATE>
      <TEMPLATE NAME="DI" COMPONENT="sDatastoreIn" />
      <TEMPLATE_PARAM SOURCE="self.UNIQUE_NAME" TARGET="DI.ORIGIN" />      

  Note: 
   * Name of previous component of sDsIn MUST be AGG for now.
   * ORIGIN param MUST be set to unique name of first component using TEMPLATE_PARAM.
  -->
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Fxp"
    RELEASE_DATE="20090102"
    STARTABLE="true"
    VISIBLE="false"
  >
    <SIGNATURE/>
  </HEADER>
  <FAMILIES>
    <FAMILY>Technical</FAMILY>
  </FAMILIES> 
  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="ORIGIN"
      FIELD="TEXT"
      NUM_ROW="2"
      REQUIRED="true"
    >
      <DEFAULT>array_tArray_1</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="4"
    >
      <DEFAULT/> 
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>