      <TEMPLATE NAME="DI" COMPONENT="sDatastoreIn" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA_OUT" TARGET="AGG.SCHEMA" />
      <TEMPLATE_PARAM SOURCE="self.DESTINATION" TARGET="AGG.DESTINATION" />
      <TEMPLATE_PARAM SOURCE="self.DISTANCE" TARGET="AGG.DISTANCE" />
      <TEMPLATE_PARAM SOURCE="self.MIN" TARGET="AGG.MIN" />
      <TEMPLATE_PARAM SOURCE="self.MAX" TARGET="AGG.MAX" />
      <TEMPLATE_PARAM SOURCE="self.OPERATION" TARGET="AGG.OPERATION" />
      <TEMPLATE_PARAM SOURCE="self.UNIQUE_NAME" TARGET="DI.ORIGIN" />      
    </TEMPLATES>
