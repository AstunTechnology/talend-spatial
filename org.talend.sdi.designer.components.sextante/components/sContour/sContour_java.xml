  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="FLOW" NAME="CONTOUR" MAX_OUTPUT="1" MAX_INPUT="0" BASE_SCHEMA="CONTOUR"/>
    <CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="1" MAX_OUTPUT="0"/>    
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1" />
  </CONNECTORS>
  <PARAMETERS>
    <PARAMETER NAME="OPERATION" FIELD="TEXT" REQUIRED="false" SHOW="false" 
      NUM_ROW="1">
      <DEFAULT>contour</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="DISTANCE" FIELD="TEXT" REQUIRED="false"
      NUM_ROW="3">
      <DEFAULT>100</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="MIN" FIELD="TEXT" REQUIRED="true" NUM_ROW="4">
      <DEFAULT>0</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="MAX" FIELD="TEXT" REQUIRED="true" NUM_ROW="4">
      <DEFAULT>10000</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SCHEMA_OUT" FIELD="SCHEMA_TYPE" REQUIRED="true"
      NUM_ROW="5" READONLY="true" CONTEXT="CONTOUR">
      <TABLE>
        <COLUMN NAME="ID" TYPE="id_Integer" CUSTOM="false"
          READONLY="true" />
        <COLUMN NAME="VALUE" TYPE="id_Double" CUSTOM="false"
          READONLY="true" />
        <COLUMN NAME="the_geom" TYPE="id_Geometry" CUSTOM="false"
          READONLY="true" />
      </TABLE>
    </PARAMETER>
    <PARAMETER NAME="DESTINATION" FIELD="TEXT" NUM_ROW="4" SHOW="false"
      REQUIRED="true">
      <DEFAULT>sContour_1</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  <CODEGENERATION>
