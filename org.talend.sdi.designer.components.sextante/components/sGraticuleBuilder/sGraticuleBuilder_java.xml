 <COMPONENT>
  <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
    COMPATIBILITY="ALL" AUTHOR="Fxp" RELEASE_DATE="20080829"
    STARTABLE="true">
    <SIGNATURE></SIGNATURE>
  </HEADER>
  <FAMILIES>
    <FAMILY>Geo/File/Input</FAMILY>
  </FAMILIES>  
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
  <CONNECTORS>
    <CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" MAX_OUTPUT="0" />
    <CONNECTOR CTYPE="FLOW" MAX_OUTPUT="1" MAX_INPUT="0" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
  </CONNECTORS>
  <PARAMETERS>
    <PARAMETER NAME="XMIN" FIELD="TEXT" REQUIRED="false"
      NUM_ROW="1">
      <DEFAULT>-180</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="YMIN" FIELD="TEXT" REQUIRED="false"
      NUM_ROW="1">
      <DEFAULT>-90</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="XMAX" FIELD="TEXT" REQUIRED="false"
      NUM_ROW="2">
      <DEFAULT>180</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="YMAX" FIELD="TEXT" REQUIRED="false"
      NUM_ROW="2">
      <DEFAULT>90</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="INTERVALX" FIELD="TEXT" REQUIRED="false"
      NUM_ROW="3">
      <DEFAULT>1</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="INTERVALY" FIELD="TEXT" REQUIRED="false"
      NUM_ROW="4">
      <DEFAULT>1</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="TYPE" FIELD="CLOSED_LIST" REQUIRED="true"
      NUM_ROW="5">
      <ITEMS DEFAULT="1">
        <!-- <ITEM NAME="RECTANGLE" VALUE="0" />
        <ITEM NAME="POINT" VALUE="2" /
        FIXME Turned off cos' don't work but don't really know why ? 
         -->
        <ITEM NAME="LINE" VALUE="1" />
      </ITEMS>
    </PARAMETER>    
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="12"
      READONLY="true"
    >
      <TABLE READONLY="false">
        <COLUMN NAME="ID" TYPE="id_Integer" READONLY="true" CUSTOM="true"/>
        <COLUMN NAME="the_geom" TYPE="id_Geometry" READONLY="true" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>
  </PARAMETERS>
  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="bsh" MODULE="bsh-2.0b4.jar" REQUIRED="true" />
      <IMPORT NAME="jcommon" MODULE="jcommon-1.0.14.jar" REQUIRED="true" />
      <IMPORT NAME="jep" MODULE="jep-2.4.0.jar" REQUIRED="true" />
      <IMPORT NAME="jfreechart" MODULE="jfreechart-1.0.11.jar" REQUIRED="true" />
      <IMPORT NAME="jgraph" MODULE="jgraph.jar" REQUIRED="true" />
      <IMPORT NAME="jts" MODULE="jts-1.9.jar" REQUIRED="true" />
      <IMPORT NAME="kxml" MODULE="kxml2.jar" REQUIRED="true" />
      <IMPORT NAME="libDocEngines" MODULE="libDocEngines-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="libMath" MODULE="libMath-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante" MODULE="sextante-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_gridAnalysis" MODULE="sextante_gridAnalysis-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_gridCalculus" MODULE="sextante_gridCalculus-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_gridCategorical" MODULE="sextante_gridCategorical-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_gridStatistics" MODULE="sextante_gridStatistics-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_gridTools" MODULE="sextante_gridTools-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_gt25" MODULE="sextante_gt25_bindings.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_gui" MODULE="sextante_gui-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_hydrology" MODULE="sextante_hydrology-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_imageAnalysis" MODULE="sextante_imageAnalysis-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_lighting" MODULE="sextante_lighting-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_locate" MODULE="sextante_locate-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_morphometry" MODULE="sextante_morphometry-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_pointAnalysis" MODULE="sextante_pointAnalysis-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_profiles" MODULE="sextante_profiles-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_rasterize" MODULE="sextante_rasterize-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_statisticalMethods" MODULE="sextante_statisticalMethods-0.15.jar" REQUIRED="true" />
      <IMPORT NAME="sextante_tables" MODULE="sextante_tables-0.15.jar" REQUIRED="true" />
