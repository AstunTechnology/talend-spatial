<COMPONENT>
  <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
    COMPATIBILITY="ALL" AUTHOR="Fxp" RELEASE_DATE="20100317"
    STARTABLE="true">
    <SIGNATURE></SIGNATURE>
  </HEADER>
  <FAMILIES>
    <FAMILY>Geo/File/Management</FAMILY>
  </FAMILIES>
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MAX_INPUT="0" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>
  <PARAMETERS>
    <PARAMETER NAME="ACTION" FIELD="CLOSED_LIST"
      REQUIRED="false" NUM_ROW="1">
      <ITEMS>
        <ITEM NAME="INSERT_OR_UPDATE" VALUE="INSERT_OR_UPDATE" />
        <ITEM NAME="DELETE_FEATURETYPE" VALUE="DELETE_FEATURETYPE" />
        <ITEM NAME="DELETE_COVERAGE" VALUE="DELETE_COVERAGE" />
      </ITEMS>
    </PARAMETER>
    <PARAMETER NAME="DATASTORE_FILE" FIELD="FILE" NUM_ROW="1"
      REQUIRED="true" SHOW_IF="(ACTION == 'INSERT_OR_UPDATE')">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/shapefile.zip"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="FEATURETYPE" FIELD="TEXT" NB_LINES="1" NUM_ROW="2"
      REQUIRED="false" SHOW_IF="(ACTION != 'INSERT_OR_UPDATE')">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="NS" FIELD="TEXT" NUM_ROW="3"
      REQUIRED="false">
      <DEFAULT>"topp"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="URL" FIELD="TEXT" NUM_ROW="4"
      REQUIRED="false">
      <DEFAULT>"http://localhost:8080/geoserver/rest"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="USERNAME" FIELD="TEXT" NB_LINES="3" NUM_ROW="5"
      REQUIRED="false">
      <DEFAULT>"admin"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="PASSWORD" FIELD="TEXT" NB_LINES="3" NUM_ROW="5"
      REQUIRED="false">
      <DEFAULT>"geoserver"</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="org.talend.sdi" MODULE="org.talend.sdi.jar"
        REQUIRED="true" />
      <IMPORT NAME="commons-httpclient" MODULE="commons-httpclient-3.0.1.jar"
        REQUIRED="true" />
      <IMPORT NAME="commons-logging" MODULE="commons-logging-1.1.1.jar"
        REQUIRED="true" />
      <IMPORT NAME="commons-codec" MODULE="commons-codec-1.3.jar"
        REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
  <RETURNS>
    <RETURN NAME="STATUS" TYPE="id_String" AVAILABILITY="AFTER" />
  </RETURNS>
</COMPONENT>
