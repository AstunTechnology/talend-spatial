<COMPONENT>
  <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
    COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
    STARTABLE="false" DATA_AUTO_PROPAGATE="false"
    SCHEMA_AUTO_PROPAGATE="true">
    <SIGNATURE />
  </HEADER>
  <FAMILIES>
    <FAMILY>Geo/File/Output</FAMILY>
  </FAMILIES>
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>
  <PARAMETERS>
    <PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="1" REQUIRED="true"
      REPOSITORY_VALUE="FILE_PATH">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.xml"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="TITLE" FIELD="TEXT" REQUIRED="true" NUM_ROW="3">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="DESCRIPTION" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="4">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="PUBDATE" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="5">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="LINK" FIELD="TEXT" REQUIRED="true" NUM_ROW="6">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
      NUM_ROW="7">
    </PARAMETER>
    <PARAMETER NAME="GEOFORMAT" FIELD="CLOSED_LIST" NUM_ROW="8">
      <ITEMS DEFAULT="SIMPLEGEORSS">
        <ITEM NAME="SIMPLEGEORSS" VALUE="SIMPLEGEORSS" />
        <ITEM NAME="GMLPOINT" VALUE="GMLPOINT" />
        <!--        Not yet supported <ITEM NAME="GML" VALUE="GML" />-->
      </ITEMS>
    </PARAMETER>
  </PARAMETERS>
  <ADVANCED_PARAMETERS>
    <PARAMETER NAME="APPEND" FIELD="CHECK" REQUIRED="true"
      NUM_ROW="2">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="2"
      REQUIRED="true" REPOSITORY_VALUE="ENCODING">
      <DEFAULT>"UTF-8"</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>
  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="Java-DOM4J" MODULE="dom4j-1.6.1.jar"
        REQUIRED="true" />
      <IMPORT NAME="Java-JAXEN" MODULE="jaxen-1.1.1.jar"
        REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
  </RETURNS>
</COMPONENT>