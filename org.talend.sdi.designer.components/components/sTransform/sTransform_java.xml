<COMPONENT>
  <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
    COMPATIBILITY="ALL" AUTHOR="fxp" RELEASE_DATE="20070829"
    STARTABLE="false" DATA_AUTO_PROPAGATE="false"
    SCHEMA_AUTO_PROPAGATE="true">
    <SIGNATURE></SIGNATURE>
  </HEADER>
  <FAMILIES>
    <FAMILY>Geo/Manipulators</FAMILY>
  </FAMILIES>
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_OUTPUT="1" MAX_INPUT="1" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>
  <PARAMETERS>
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
      NUM_ROW="1">
      <TABLE>
      </TABLE>
    </PARAMETER>
    <PARAMETER NAME="TX" FIELD="TEXT"
      NUM_ROW="2">
      <DEFAULT></DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="TY" FIELD="TEXT"
      NUM_ROW="2">
      <DEFAULT></DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="ROTATION" FIELD="TEXT"
      NUM_ROW="3">
      <DEFAULT></DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="USE_CENTROID_AS_ANCHOR" FIELD="CHECK" REQUIRED="true"
      NUM_ROW="4">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="AX" FIELD="TEXT" SHOW_IF="USE_CENTROID_AS_ANCHOR == 'false'"
      NUM_ROW="4">
      <DEFAULT></DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="AY" FIELD="TEXT" SHOW_IF="USE_CENTROID_AS_ANCHOR == 'false'"
      NUM_ROW="4">
      <DEFAULT></DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SX" FIELD="TEXT"
      NUM_ROW="6">
      <DEFAULT></DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="SY" FIELD="TEXT"
      NUM_ROW="6">
      <DEFAULT></DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="OPERATION_TYPE" FIELD="TEXT" REQUIRED="true"
      NUM_ROW="2" SHOW="false">
      <DEFAULT>TRANSFORM</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  <ADVANCED_PARAMETERS>
	<PARAMETER NAME="IMPORT" FIELD="MEMO_IMPORT" REQUIRED="false" SHOW="false" NUM_ROW="1">
	  <DEFAULT>import org.talend.sdi.geometry.Geometry;</DEFAULT>
	</PARAMETER>   
  </ADVANCED_PARAMETERS>
  <CODEGENERATION>
    <TEMPLATES INPUT="BO" OUTPUT="FF">
      <TEMPLATE NAME="BO" COMPONENT="sGeoBasicOperation">
        <LINK_TO NAME="FF" CTYPE="FLOW" />
      </TEMPLATE>
      <TEMPLATE NAME="FF" COMPONENT="sFlowForwarder" />
      <TEMPLATE_PARAM SOURCE="self.OPERATION_TYPE"
        TARGET="BO.OPERATION_TYPE" />
      <TEMPLATE_PARAM SOURCE="self.TX" TARGET="BO.TX" />
      <TEMPLATE_PARAM SOURCE="self.TY" TARGET="BO.TY" />
      <TEMPLATE_PARAM SOURCE="self.AX" TARGET="BO.AX" />
      <TEMPLATE_PARAM SOURCE="self.AY" TARGET="BO.AY" />
      <TEMPLATE_PARAM SOURCE="self.SX" TARGET="BO.SX" />
      <TEMPLATE_PARAM SOURCE="self.SY" TARGET="BO.SY" />
      <TEMPLATE_PARAM SOURCE="self.USE_CENTROID_AS_ANCHOR" TARGET="BO.USE_CENTROID_AS_ANCHOR" />
      <TEMPLATE_PARAM SOURCE="self.ROTATION" TARGET="BO.ROTATION" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="BO.SCHEMA" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="FF.SCHEMA" />
    </TEMPLATES>
  </CODEGENERATION>
  <RETURNS>
    <RETURN NAME="NB_FEATURE" TYPE="INTEGER" AVAILABILITY="AFTER" />
  </RETURNS>
</COMPONENT>
